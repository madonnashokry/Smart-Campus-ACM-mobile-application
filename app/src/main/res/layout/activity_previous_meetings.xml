<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F5F5F5">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context=".Previous_Meetings">

        <TextView
            android:id="@+id/meetings_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Previous Meetings"
            android:textColor="#626A6A"
            android:textSize="27dp"
            android:textStyle="bold"
            android:layout_marginTop="25dp"
            android:gravity="center"
            app:layout_constraintTop_toTopOf="@+id/main"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/calendar"
            app:layout_constraintTop_toTopOf="@+id/main"
            app:layout_constraintEnd_toEndOf="@+id/main"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="16dp" />

        <TextView
            android:id="@+id/date_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Today      Sunday, Feb 24,2024"
            android:textSize="20dp"
            android:textStyle="bold"
            android:textColor="#919595"
            android:layout_marginTop="25dp"
            app:layout_constraintTop_toBottomOf="@id/meetings_text"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <View
            android:id="@+id/rectangle_5_ek2"
            android:layout_width="327dp"
            android:layout_height="126dp"
            android:layout_marginTop="28dp"
            android:background="@drawable/rounded_rec2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/date_text" />

        <TextView
            android:id="@+id/embedded_systems"
            android:layout_width="190.8dp"
            android:layout_height="37.8dp"
            android:layout_marginTop="20dp"
            android:text="Embedded Systems"
            android:textColor="#626A6A"
            android:textSize="20dp"
            android:textStyle="normal|bold"
            app:layout_constraintEnd_toEndOf="@id/rectangle_5_ek2"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@id/rectangle_5_ek2"
            app:layout_constraintTop_toTopOf="@id/rectangle_5_ek2" />

        <TextView
            android:id="@+id/hall_1_ground_floor"
            android:layout_width="107dp"
            android:layout_height="42.6dp"
            android:layout_gravity="center_horizontal|center"
            android:layout_marginTop="8dp"
            android:gravity="center"
            android:text="Hall 1\nGround Floor"
            android:textColor="#919595"
            android:textSize="18dp"
            android:textStyle="normal|bold"
            app:layout_constraintEnd_toEndOf="@id/rectangle_5_ek2"
            app:layout_constraintHorizontal_bias="0.772"
            app:layout_constraintStart_toStartOf="@id/rectangle_5_ek2"
            app:layout_constraintTop_toBottomOf="@id/embedded_systems" />

        <TextView
            android:id="@+id/_08_00_am___10_00_am"
            android:layout_width="110dp"
            android:layout_height="80dp"
            android:layout_marginTop="20dp"
            android:text="08:00   AM-\n10:00   AM"
            android:textColor="#919595"
            android:textSize="20sp"
            android:textStyle="normal|bold"
            app:layout_constraintEnd_toEndOf="@id/rectangle_5_ek2"
            app:layout_constraintHorizontal_bias="0.046"
            app:layout_constraintStart_toStartOf="@id/rectangle_5_ek2"
            app:layout_constraintTop_toTopOf="@id/rectangle_5_ek2" />

        <View
            android:layout_width="327dp"
            android:layout_height="126dp"
            android:layout_marginTop="180dp"
            android:background="@drawable/rounded_rec2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/date_text" />

        <TextView

            android:layout_width="190.8dp"
            android:layout_height="37.8dp"
            android:layout_marginTop="170dp"
            android:text="Real-Time Systems"
            android:textColor="#626A6A"
            android:textSize="20dp"
            android:textStyle="normal|bold"
            app:layout_constraintEnd_toEndOf="@id/rectangle_5_ek2"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@id/rectangle_5_ek2"
            app:layout_constraintTop_toTopOf="@id/rectangle_5_ek2" />

        <TextView

            android:layout_width="107dp"
            android:layout_height="42.6dp"
            android:layout_gravity="center_horizontal|center"
            android:layout_marginTop="160dp"
            android:gravity="center"
            android:text="Hall 2\nFirst Floor"
            android:textColor="#919595"
            android:textSize="18dp"
            android:textStyle="normal|bold"
            app:layout_constraintEnd_toEndOf="@id/rectangle_5_ek2"
            app:layout_constraintHorizontal_bias="0.772"
            app:layout_constraintStart_toStartOf="@id/rectangle_5_ek2"
            app:layout_constraintTop_toBottomOf="@id/embedded_systems" />

        <TextView
            android:layout_width="110dp"
            android:layout_height="80dp"
            android:layout_marginTop="170dp"
            android:text="10:00   AM-\n12:00   PM"
            android:textColor="#919595"
            android:textSize="20sp"
            android:textStyle="normal|bold"
            app:layout_constraintEnd_toEndOf="@id/rectangle_5_ek2"
            app:layout_constraintHorizontal_bias="0.046"
            app:layout_constraintStart_toStartOf="@id/rectangle_5_ek2"
            app:layout_constraintTop_toTopOf="@id/rectangle_5_ek2" />

        <View
            android:layout_width="327dp"
            android:layout_height="126dp"
            android:layout_marginTop="335dp"
            android:background="@drawable/rounded_rec2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/date_text" />

        <TextView

            android:layout_width="190.8dp"
            android:layout_height="37.8dp"
            android:layout_marginTop="325dp"
            android:text="Mobile Computing"
            android:textColor="#626A6A"
            android:textSize="20dp"
            android:textStyle="normal|bold"
            app:layout_constraintEnd_toEndOf="@id/rectangle_5_ek2"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@id/rectangle_5_ek2"
            app:layout_constraintTop_toTopOf="@id/rectangle_5_ek2" />

        <TextView

            android:layout_width="107dp"
            android:layout_height="42.6dp"
            android:layout_gravity="center_horizontal|center"
            android:layout_marginTop="315dp"
            android:gravity="center"
            android:text="Hall 3\nSecond Floor"
            android:textColor="#919595"
            android:textSize="18dp"
            android:textStyle="normal|bold"
            app:layout_constraintEnd_toEndOf="@id/rectangle_5_ek2"
            app:layout_constraintHorizontal_bias="0.772"
            app:layout_constraintStart_toStartOf="@id/rectangle_5_ek2"
            app:layout_constraintTop_toBottomOf="@id/embedded_systems" />

        <TextView
            android:layout_width="110dp"
            android:layout_height="80dp"
            android:layout_marginTop="325dp"
            android:text="02:00   AM-\n04:00   PM"
            android:textColor="#919595"
            android:textSize="20sp"
            android:textStyle="normal|bold"
            app:layout_constraintEnd_toEndOf="@id/rectangle_5_ek2"
            app:layout_constraintHorizontal_bias="0.046"
            app:layout_constraintStart_toStartOf="@id/rectangle_5_ek2"
            app:layout_constraintTop_toTopOf="@id/rectangle_5_ek2" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>